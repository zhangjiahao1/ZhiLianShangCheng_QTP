'个人信息
Function InformationTest()
Browser("智联商城").Page("智联商城").Link("登录").Click
Browser("智联商城").Page("登录").WebEdit("username").Set "zhangjiahao"
Browser("智联商城").Page("登录").WebEdit("password").Set "123456789"
Browser("智联商城").Page("登录").WebButton("登  录").Click
Browser("智联商城").Page("智联商城_2").Link("zhangjiahao").Click
Browser("智联商城").Page("京东12").WebEdit("nickname").Set ReadExcel("C:/QTP_Eagle/Data/Information.xlsx", "Sheet1", 2, 1)
Browser("智联商城").Page("京东12").WebEdit("email").Set ReadExcel("C:/QTP_Eagle/Data/Information.xlsx", "Sheet1", 2, 2)
Browser("智联商城").Page("京东12").WebEdit("username").Set ReadExcel("C:/QTP_Eagle/Data/Information.xlsx", "Sheet1", 2, 3)
Browser("智联商城").Page("京东12").WebEdit("password").Set ReadExcel("C:/QTP_Eagle/Data/Information.xlsx", "Sheet1", 2, 4)
Browser("智联商城").Page("京东12").WebEdit("repassword").Set ReadExcel("C:/QTP_Eagle/Data/Information.xlsx", "Sheet1", 2, 5)
Browser("智联商城").Page("京东12").WebButton("提交").Click
If Browser("智联商城").Page("京东12").Link("zhangjiahao").Exist Then
	CheckValue="修改成功"
	Call DoExcelColorGreen("C:/QTP_Eagle/Data/Information.xlsx","Sheet1",2,6)
	Call DoExcel("C:/QTP_Eagle/Data/Information.xlsx","Sheet1",2,6)
	Call logPrint("成功修改个人信息")
