	Call DoExcelColorRed("C:/QTP_Eagle/Data/Information.xlsx","Sheet1",2,6)
	Call DoExcel("C:/QTP_Eagle/Data/Information.xlsx","Sheet1",2,6)
	Call logPrint("修改个人信息失败")
End If
Browser("智联商城").CloseAllTabs
End Function


